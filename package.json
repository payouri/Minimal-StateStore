{
  "name": "@youri-kane/minimal-statestore",
  "version": "1.4.0",
  "description": "a minimal state data store, able to validate data before storage and to fire callbacks on data update",
  "main": "dist/minimal-statestore.js",
  "module": "src/index.js",
  "unpkg": "dist/minimal-statestore.js",
  "scripts": {
    "build": "node ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js ./src/index.js",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "test": "jest",
    "test-verbose": "jest --coverage --config ./jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payouri/Minimal-StateStore.git"
  },
  "keywords": [
    "state",
    "simple"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "author": "Youri Pailloux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/payouri/Minimal-StateStore/issues"
  },
  "homepage": "https://github.com/payouri/Minimal-StateStore#readme",
  "dependencies": {}
}
